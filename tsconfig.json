{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"~/*": ["./lib/*"]
		},
		"plugins": [{"name": "next"}]
	},
	"exclude": ["node_modules"],
	"extends": "@rubriclab/typescript-config/nextjs",
	"include": [
		".next/types/**/*.ts",
		".eslintrc.cjs",
		"next-env.d.ts",
		"**/*.ts",
		"**/*.tsx"
	]
}
